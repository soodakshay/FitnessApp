<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fitness">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_splash"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SignupActivity"></activity>
        <activity android:name=".Activity.BaseActivity"></activity>
        <activity android:name=".Activity.LooseActivity"></activity>
        <activity android:name=".Activity.LooseExerciseActivity"></activity>
        <activity android:name=".Activity.TrainingTypeActivity"></activity>
        <activity android:name=".Activity.WorkoutListActivity"></activity>
        <activity android:name=".Activity.WorkoutListItem"></activity>
        <activity android:name=".Activity.LoseDietWorkout"></activity>
        <activity android:name=".Activity.LooseDiet"></activity>
        <activity android:name=".Gain.GainDiet"></activity>
        <activity android:name=".Gain.GainDietWorkout"></activity>
        <activity android:name=".Gain.GainWorkoutChart"></activity>
        <activity android:name=".Activity.MainActivity"></activity>
    </application>

</manifest>